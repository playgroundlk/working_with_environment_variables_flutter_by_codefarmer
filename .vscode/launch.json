  "version": "0.2.0",
  "configurations": [
    {
      "name": "development",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define-from-file",
        "lib/env/development.env"
      ]
    },
    {
      "name": "production",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": [
        "--dart-define-from-file",
        "lib/env/production.env"
      ]
    },
    {
      "name": "working_with_environment_variables_flutter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "working_with_environment_variables_flutter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}